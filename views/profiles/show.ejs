<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/profiles/show.css" />
<%- include('../partials/nav') %>

<main>
  <div id="name"><%= profile.name %></div>
  <div> 
    <img id="avatar"
      class="profile-avatar"
      src="<%=  profile.avatar %>" 
      alt="<%= profile.name %>"
    >
  </div>
  <div id="like-games">Games liked: 
    <% profile.likeGames.forEach(game => { %>
    <p><%= game.title %></p>
    <% }) %>
  <div>
  <div class="games-created"> 
    <p>Games <%= profile.name %>'s' created:</p>
    <% games.forEach(game => { %>
      <p><%= game.title %></p>
    <% }) %>
  </div>
</main>

<%- include('../partials/footer') %>